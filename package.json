{
  "name": "@samducker/payload-task",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 4321 --turbo",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "pg:up": "docker-compose up -d",
    "seed": "pnpm run payload migrate:fresh",
    "pg:down": "docker-compose down",
    "pg:build": "docker-compose up --build",
    "drizzle:studio": "drizzle-kit studio --port=8143"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.24.0",
    "@payloadcms/live-preview-react": "3.24.0",
    "@payloadcms/next": "3.24.0",
    "@payloadcms/payload-cloud": "3.24.0",
    "@payloadcms/plugin-form-builder": "3.24.0",
    "@payloadcms/plugin-multi-tenant": "3.24.0",
    "@payloadcms/plugin-nested-docs": "3.24.0",
    "@payloadcms/plugin-redirects": "3.24.0",
    "@payloadcms/plugin-search": "3.24.0",
    "@payloadcms/plugin-seo": "3.24.0",
    "@payloadcms/richtext-lexical": "3.24.0",
    "@payloadcms/ui": "3.24.0",
    "@radix-ui/react-checkbox": "1.1.4",
    "@radix-ui/react-label": "2.1.2",
    "@radix-ui/react-select": "2.1.6",
    "@radix-ui/react-slot": "1.1.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cross-env": "7.0.3",
    "drizzle-kit": "^0.30.4",
    "drizzle-orm": "^0.39.3",
    "geist": "1.3.1",
    "graphql": "16.10.0",
    "lucide-react": "0.378.0",
    "next": "15.1.7",
    "next-sitemap": "4.2.3",
    "payload": "3.24.0",
    "payload-admin-bar": "1.0.6",
    "pg": "8.11.3",
    "prism-react-renderer": "2.4.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.45.4",
    "sharp": "0.32.6",
    "tailwind-merge": "2.6.0",
    "tailwindcss-animate": "1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.0",
    "@tailwindcss/typography": "0.5.16",
    "@types/escape-html": "1.0.4",
    "@types/node": "22.5.4",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "10.4.20",
    "copyfiles": "2.4.1",
    "eslint": "9.21.0",
    "eslint-config-next": "15.1.7",
    "postcss": "8.5.3",
    "prettier": "3.5.2",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ],
    "overrides": {
      "drizzle-orm": "0.39.3"
    }
  }
}